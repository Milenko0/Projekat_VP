<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<appSettings>
		<add key="CsvFolderPath" value="..\..\..\Fajlovi\csv_potrosnja_za_danasnji_dan_primeri"/>
		<add key="TxtFolderPath" value="..\..\..\Fajlovi\txt_rezultati_primeri"/>
	</appSettings>
	
	<system.serviceModel>
		<services>
			<service name="Server.ServerService">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4000" />
					</baseAddresses>
				</host>
				<endpoint address="IServerService" binding="netTcpBinding" contract="Common.IServerService" />
			</service>
		</services>
	</system.serviceModel>
	
</configuration>