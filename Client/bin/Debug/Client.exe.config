<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<appSettings>
		<add key="TxtFolderPath" value="..\..\..\Fajlovi\txt_rezultati_primeri"/>
		<add key="storageType" value="FileSystem"/>
		<!-- FileSystem / DB -->
		<add key="downloadPath" value="Download"/>
		<add key="uploadPath" value="Upload"/>
		<add key="uploaderType" value="Event"/>
		<!-- Background / Event -->
	</appSettings>

	<system.serviceModel>
		<client>
			<endpoint name="ServiceServer"
					  address="net.tcp://localhost:4000/ServiceServer"
					  binding="netTcpBinding"
					  contract="Common.IServerService"/>
		</client>
	</system.serviceModel>
	
</configuration>